{
  "name": "meteor_boilerplate",
  "version": "0.0.1",
  "description": "A simple MeteorJS project boilerplate.",
  "main": "main.js",
  "scripts": {
    "start": "meteor run",
    "lint": "eslint .",
    "pretest": "npm run lint --silent"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andyp22/meteor_boilerplate.git"
  },
  "keywords": [
    "meteor",
    "boilerplate"
  ],
  "author": "Andrew Page",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andyp22/meteor_boilerplate/issues"
  },
  "homepage": "https://github.com/andyp22/meteor_boilerplate",
  "dependencies": {
    "meteor-node-stubs": "~0.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-import-resolver-meteor": "^0.2.4",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-meteor": "^3.5.2",
    "eslint-plugin-react": "^5.2.2"
  },
  "peerDependencies": {
    "eslint": ">=0.8.0"
